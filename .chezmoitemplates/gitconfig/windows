	name = rioka
	email = rioka@libero.it
[credential "https://github.com"]
	provider = github
[gpg]
	format = ssh
[gpg "ssh"]
	program = C:/Windows/System32/OpenSSH/ssh-keygen.exe
[commit]
	gpgSign = false
[pull]
	rebase = false
[gui]
	pruneduringfetch = true
[push]
	recurseSubmodules = check
[rerere]
	enabled = true
